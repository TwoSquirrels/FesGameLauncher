%><%
  page.top = "../".repeat((page.path.match(/\//g) ?? []).length);
  page.title = `${site.name} | ${page.title}`;
  const extra = {
    path: page.extra ?? "",
    top: "../".repeat(page.extra ? ((page.extra.match(/\//g) ?? []).length + 1) : 0),
  };
%><!DOCTYPE html>

<!---
<%= `${site.name} v${site.version}` %>
<%= site.message %>
--->

<html lang="ja">

  <head prefix="og: http://ogp.me/ns#">

    <!-- meta -->
    <meta charset="utf-8" />
    <meta http-equiv="content-language" content="ja" />
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes" />
    <meta name="Content-Style-Type" content="text/css" />
    <meta name="keywords" content="<%= page.keywords.join() %>" />
    <meta name="description" content="<%= page.description %>" />
    <meta name="author" content="<%= page.author %>" />
    <meta name="theme-color" content="#<%= page.color %>" />
    <meta name="apple-mobile-web-app-status-bar-style" content="#<%= page.color %>" />

    <!-- ogp -->
    <meta property="og:url" content="<%= site.url %><%= page.path === "index" ? "" : page.path  %>" />
    <meta property="og:type" content="<%= page.type %>" />
    <meta property="og:title" content="<%= page.title %>" />
    <meta property="og:description" content="<%= page.description %>" />
    <meta property="og:site_name" content="<%= site.name %>" />
    <meta property="og:image" content="<%= site.url %>resources/<%= site.version %>/images/thumbnails/<%= page.path %>.png" />
    <!-- twitter -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="@<%= page.twitter %>" />

    <!-- main head -->
    <title><%= page.title %></title>
    <link id="favicon" rel="icon" href="<%= site.url %>resources/<%= site.version %>/images/favicon.ico" type="image/x-icon" />

    <!-- definitions -->
    <script>
      const constants = JSON.parse(`<%- JSON.stringify({ site, page }) %>`);
      const extra = {
        path: "<%= extra.path %>",
        top() {
          return "../".repeat(this.path ? ((this.path.match(/\//g) ?? []).length + 1) : 0);
        },
      };
    </script>

    <!-- stylesheet -->
    <!-- site common -->
    <% site.libs.css.forEach(lib => { %><link rel="stylesheet" href="<%= page.top + extra.top %>resources/libs/<%= lib %>" />
    <% }); %><link rel="stylesheet" href="<%= page.top + extra.top %>resources/<%= site.version %>/style.css" />
    <!-- page specific -->
    <% page.libs.css.forEach(lib => { %><link rel="stylesheet" href="<%= page.top + extra.top %>resources/libs/<%= lib %>" />
    <% }); %><link rel="stylesheet" href="<%= page.top + extra.top %>resources/<%= site.version %>/<%= page.path %>.css" />

    <!-- javascript -->
    <!-- site common -->
    <% site.libs.js.forEach(lib => { %><script src="<%= page.top + extra.top %>resources/libs/<%= lib %>" defer></script>
    <% }); %><script src="<%= page.top + extra.top %>resources/<%= site.version %>/script.js" charset="utf-8" defer></script>
    <!-- page specific -->
    <% page.libs.js.forEach(lib => { %><script src="<%= page.top + extra.top %>resources/libs/<%= lib %>" defer></script>
    <% }); %><script src="<%= page.top + extra.top %>resources/<%= site.version %>/<%= page.path %>.js" charset="utf-8" defer></script>

  </head>

  <body>

<%- include("../../" + render.body, { render, site, page, extra }) %>
    <script>
